{
  "name": "server",
  "version": "1.0.0",
  "description": "Node.js + Express + TypeScript API server",
  "main": "dist/main.js",
  "scripts": {
    "dev": "nodemon src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "start:prod": "node dist/main.js",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "worker:scrape": "ts-node src/workers/scrapeWorker.ts",
    "worker:ai": "ts-node src/workers/aiWorker.ts"
  },
  "dependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@types/dotenv": "^6.1.1",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.4.1",
    "morgan": "^1.10.0",
    "puppeteer": "^24.24.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.19.21",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
