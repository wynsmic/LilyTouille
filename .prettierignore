# Prettier ignore patterns
# Ignore build outputs
dist/
build/
*.min.js
*.min.css

# Ignore package files
package-lock.json
yarn.lock
pnpm-lock.yaml

# Ignore generated files
*.d.ts
*.js.map

# Ignore database files
*.db
*.sqlite

# Ignore logs
*.log
logs/

# Ignore coverage reports
coverage/

# Ignore node_modules
node_modules/

# Ignore environment files
.env
.env.local
.env.production
.env.development
